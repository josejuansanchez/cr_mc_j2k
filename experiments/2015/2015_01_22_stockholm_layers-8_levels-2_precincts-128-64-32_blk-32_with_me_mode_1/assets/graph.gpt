#set terminal fig textspecial color
set terminal png
set output "psnr.png"

set title ''
set xlabel 'Frame Number'
set ylabel 'PSNR [dB]'

#set yrange [0:60]
set key out right

plot "paste_11000.txt" using 1:3 with lines title 'cr + mc (11000 Bytes)' axes x1y1, \
     "paste_21000.txt" using 1:3 with lines title 'cr + mc (21000 Bytes)' axes x1y1, \
     "paste_31000.txt" using 1:3 with lines title 'cr + mc (31000 Bytes)' axes x1y1, \
     "paste_41000.txt" using 1:3 with lines title 'cr + mc (41000 Bytes)' axes x1y1, \
     "paste_51000.txt" using 1:3 with lines title 'cr + mc (51000 Bytes)' axes x1y1, \
     "paste_11000.txt" using 1:4 with linespoints ls 1 title 'trunc (11000 Bytes)' axes x1y1, \
     "paste_21000.txt" using 1:4 with linespoints ls 2 title 'trunc (21000 Bytes)' axes x1y1, \
     "paste_31000.txt" using 1:4 with linespoints ls 3 title 'trunc (31000 Bytes)' axes x1y1, \
     "paste_41000.txt" using 1:4 with linespoints ls 4 title 'trunc (41000 Bytes)' axes x1y1, \
     "paste_51000.txt" using 1:4 with linespoints ls 5 title 'trunc (51000 Bytes)' axes x1y1