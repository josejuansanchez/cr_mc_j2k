set terminal png
set output "psnr_type2.png"
set title ''
set xlabel 'Frame Number'
set ylabel 'PSNR [dB]'

set key out bottom

plot "paste_1000.txt" using 1:3 with lines linecolor 1 lw 2 title 'cr+me - 1K'  axes x1y1, \
	 "paste_1000.txt" using 1:4 with linespoints ls 1 title 'trunc - 1K'  axes x1y1, \
	 "paste_11000.txt" using 1:3 with lines linecolor 2 lw 2 title 'cr+me - 11K' axes x1y1, \
	 "paste_11000.txt" using 1:4 with linespoints ls 2 title 'trunc - 11K' axes x1y1, \
	 "paste_21000.txt" using 1:3 with lines linecolor 3 lw 2 title 'cr+me - 21K' axes x1y1, \
	 "paste_21000.txt" using 1:4 with linespoints ls 3 title 'trunc - 21K' axes x1y1, \
	 "paste_31000.txt" using 1:3 with lines linecolor 4 lw 2 title 'cr+me - 31K' axes x1y1, \
	 "paste_31000.txt" using 1:4 with linespoints ls 4 title 'trunc - 31K' axes x1y1, \
	 "paste_41000.txt" using 1:3 with lines linecolor 5 lw 2 title 'cr+me - 41K' axes x1y1, \
	 "paste_41000.txt" using 1:4 with linespoints ls 5 title 'trunc - 41K' axes x1y1, \
	 "paste_51000.txt" using 1:3 with lines linecolor 6 lw 2 title 'cr+me - 51K' axes x1y1, \
	 "paste_51000.txt" using 1:4 with linespoints ls 6 title 'trunc - 51K' axes x1y1
