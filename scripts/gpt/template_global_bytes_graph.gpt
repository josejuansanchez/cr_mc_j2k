#set terminal fig textspecial color
set terminal png
set output "bytes.png"

set title ''
set xlabel 'Frame Number'
set ylabel 'Bytes'

#set yrange [0:60]
set key out right

plot "paste_11000.txt" using 1:2 with lines title 'Bytes per frame (11000 Bytes)' axes x1y1, \
     "paste_21000.txt" using 1:2 with lines title 'Bytes per frame (21000 Bytes)' axes x1y1, \
     "paste_31000.txt" using 1:2 with lines title 'Bytes per frame (31000 Bytes)' axes x1y1, \
     "paste_41000.txt" using 1:2 with lines title 'Bytes per frame (41000 Bytes)' axes x1y1, \
     "paste_51000.txt" using 1:2 with lines title 'Bytes per frame (51000 Bytes)' axes x1y1
