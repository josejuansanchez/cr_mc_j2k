#set terminal fig textspecial color
set terminal png
set output "psnr.png"

set title ''
set xlabel 'Frame Number'
set ylabel 'PSNR [dB]'

#set yrange [0:60]
set key out right

plot "paste_5000.txt" using 1:3 with lines title 'T2. cr + me (5000 Bytes)' axes x1y1, \
     "paste_10000.txt" using 1:3 with lines title 'T2. cr + me (10000 Bytes)' axes x1y1, \
     "paste_15000.txt" using 1:3 with lines title 'T2. cr + me (15000 Bytes)' axes x1y1, \
     "paste_21000.txt" using 1:3 with lines title 'T2. cr + me (20000 Bytes)' axes x1y1, \
     "paste_25000.txt" using 1:3 with lines title 'T2. cr + me (25000 Bytes)' axes x1y1, \
     "paste_30000.txt" using 1:3 with lines title 'T2. cr + me (30000 Bytes)' axes x1y1, \
     "paste_5000.txt" using 1:4 with lines title 'trunc (5000 Bytes)' axes x1y1, \
     "paste_10000.txt" using 1:4 with lines title 'trunc (10000 Bytes)' axes x1y1, \
     "paste_15000.txt" using 1:4 with lines title 'trunc (15000 Bytes)' axes x1y1, \
     "paste_21000.txt" using 1:4 with lines title 'trunc (20000 Bytes)' axes x1y1, \
     "paste_25000.txt" using 1:4 with linespoints ls 2 title 'trunc (25000 Bytes)' axes x1y1, \
     "paste_30000.txt" using 1:4 with linespoints ls 3 title 'trunc (30000 Bytes)' axes x1y1     
